<%= form_with(model: [@profile], local: true) do |form| %>
  <%= form.label 'email' %>
  <%= form.hidden_field :account_id, value: current_account.id %>
  <%= form.label 'назва' %>
  <%= form.text_ :first_name %>
  <%= form.label 'назва 2' %>
  <%= form.text_ :last_name %>
  <div class="actions">
    <%= form.submit "готово" %>
  </div>
<% end %>
